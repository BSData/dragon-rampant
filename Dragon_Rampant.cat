    <entry id="ab3c-9b52-c4c5-0d1d" name="Summoner" points="3.0" categoryId="(No Category)" type="upgrade" minSelections="0" maxSelections="1" minInForce="0" maxInForce="-1" minInRoster="0" maxInRoster="-1" minPoints="0.0" maxPoints="-1.0" collective="false" hidden="false" book="Dragon Rampant - Fantasy Wargaming Rules" page="46">
      <entries/>
