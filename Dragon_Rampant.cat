    <entry id="f31e-ff64-48d4-f6e4" name="Blessed Weapons" points="2.0" categoryId="(No Category)" type="upgrade" minSelections="0" maxSelections="1" minInForce="0" maxInForce="-1" minInRoster="0" maxInRoster="-1" minPoints="0.0" maxPoints="-1.0" collective="false" hidden="false" book="Dragon Rampant - Fantasy Wargaming Rules" page="43">
